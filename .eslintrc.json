{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "no-underscore-dangle": "off",
    "class-methods-use-this": "off",
    "operator-linebreak": "off",
    "no-unused-vars": "off",
    "max-len": "off",
    "object-curly-newline": "off"
  }
}
